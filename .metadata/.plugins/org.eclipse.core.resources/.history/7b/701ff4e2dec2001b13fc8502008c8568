package model;

public class Chest {
	private Item content;
	private String descChest;
	
	public Chest(String desc, Item content){
		this.descChest = desc;
		this.content = content;
	}

	public Item getContent() {
		return content;
	}

	public void setContent(Item content) {
		this.content = content;
	}

	public String getDescChest() {
		return descChest;
	}

	public void setDescChest(String descChest) {
		this.descChest = descChest;
	}
	
	
}
